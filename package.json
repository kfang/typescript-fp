{
  "name": "@kfang/typescript-fp",
  "version": "1.25.0",
  "description": "utilities to deal with fp in typescript",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/kfang/typescript-fp",
  "license": "MIT",
  "keywords": [],
  "access": "public",
  "scripts": {
    "clean": "trash build coverage",
    "fix": "eslint --fix src tests",
    "prepack": "run-s clean prepack:*",
    "prepack:main": "tsc -p tsconfig.json",
    "prepack:module": "tsc -p tsconfig.module.json",
    "test": "run-s clean test:*",
    "test:jest": "jest",
    "test:eslint": "eslint src tests",
    "doc": "run-s doc:html",
    "doc:html": "typedoc src/index.ts --out build/docs",
    "doc:json": "typedoc src/index.ts --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "reset": "git clean -dfx && git reset --hard && yarn",
    "prepare-release": "run-s all doc:publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^14.18.32",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "semantic-release": "^19.0.5",
    "trash-cli": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2"
  }
}
